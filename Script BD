CREATE TABLE `alumno` (
  `idAlumno` int(11) NOT NULL,
  `Nombre` varchar(100) NOT NULL,
  `fecha` varchar(10) NOT NULL,
  `calificacion` int(11) NOT NULL
)
ALTER TABLE `alumno`
  ADD PRIMARY KEY (`idAlumno`);
ALTER TABLE `alumno`
  MODIFY `idAlumno` int(11) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=6;
COMMIT;
